{
  "name": "api",
  "version": "0.0.1",
  "type": "commonjs",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "engines": {
    "npm": ">=8.19.2",
    "node": ">=16.18.1 <19.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -- --source-map",
    "start:dev": "nest start --watch -- --source-map",
    "start:debug": "nest start --debug=0.0.0.0:9229 --watch -- --source-map",
    "start:prod": "node --trace-warnings --enable-source-maps dist/main",
    "migrate": "typeorm migration:run -d dist/pg.data-source.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prefixtures": "nest build",
    "fixtures": "fixtures load --dataSource=dist/pg.data-source.js fixtures",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:debug": "node --inspect-brk=0.0.0.0:9229 node_modules/.bin/jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/static": "^6.10.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.3.9",
    "@nestjs/platform-fastify": "^9.3.10",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^10.0.1",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sentry/node": "^7.30.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "fastify": "^4.14.1",
    "http-cache-semantics": "^4.1.1",
    "json-stable-stringify": "^1.0.2",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "pg": "^8.9.0",
    "qs": "^6.11.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.3.9",
    "@types/http-cache-semantics": "^4.0.1",
    "@types/jest": "^29.4.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.15.10",
    "@types/qs": "^6.9.7",
    "jest": "^29.4.3",
    "prettier": "^2.1.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typeorm-fixtures-cli": "^4.0.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
